                  <th scope="col">Adresse</th>
                  <th scope="col">Nom du médecin</th>
                  <th scope="col">Willaya</th>
                  <th scope="col">Annuler</th>
                </tr>
              </thead>
              <tbody>
                <% for(var i=0 ; i < tu.length ; i++) { %>
                <form action="" method="post">
                  <input type="hidden" name="val" value="<%= tu [i].numR %>">
                <tr>
                  <td><%= tu [i].numR %></td>
                  <td><%= tu [i].dateR %></td>
                  <td><%= tu [i].heureR %></td>
                  <td><%= tu [i].adresse %></td>
                  <td>Dr. <%= tu [i].nom %> <%= tu [i].prenom %></td>
                  <td><%= tu [i].wilaya %></td>
                  <td><Button type="submit" >Annuler</Button></td>
                
                </tr>
              </form>
              <% } %>
             
            </tbody>
          </table>
        
       
